{
  "name": "@upskill/api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:cloudflare": "wrangler dev",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "build:cloudflare": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.0",
    "@libsql/client": "^0.15.1",
    "drizzle-orm": "^0.36.3",
    "elysia": "^1.1.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.4",
    "typescript": "^5.6.3",
    "wrangler": "^4.60.0"
  }
}
